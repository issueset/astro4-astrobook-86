---
import StoryPage from '../components/StoryPage.astro'
import * as m from '../components/Welcome.stories.ts'

const isAstro = true
---

<StoryPage story={'components/welcome/blue'} hasSidebar={false}>
  {
    isAstro
      ? (<m.default.component { ...m['Blue']?.args } />)
      : (<m.default.component { ...m['Blue']?.args } client:load />)
  }
</StoryPage>

